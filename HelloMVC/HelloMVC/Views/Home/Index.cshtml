@model List<HelloMVC.Models.Car>
@{
    ViewBag.Title = "Index";

    var value = 1000;
}

@{ 
    value += 1;
}

@{ 
    var name = "Shawn";
    var day = DateTime.Now.DayOfWeek.ToString();

    <p>Howdy, @name. Today is @day</p>
    <p>Howdy, @name. Aren't @(day)s the best?</p>
}

@{ 
    var productId = 123;
    <p>Producty Id: ACME@(productId)</p>
}
<h1>@ViewBag.ShawnAddThisToo</h1>
<h1>I added this too</h1>
<h2>Shawn Changed This</h2>
<p>i added this too</p>

@foreach (var car in Model)
{
    <p>@car.Id - @car.Make - @car.Model</p>
}